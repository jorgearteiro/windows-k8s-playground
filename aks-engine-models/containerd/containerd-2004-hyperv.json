{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "Kubernetes",
      "orchestratorRelease": "1.19",
      "kubernetesConfig": {
        "networkPlugin": "azure",
        "containerRuntime": "containerd",
        "windowsContainerdURL": "https://kubernetesartifacts.azureedge.net/containerd/ms/0.0.13/binaries/containerd-windows-0.0.13.zip"
      }
    },
    "masterProfile": {
      "count": 1,
      "dnsPrefix": "",
